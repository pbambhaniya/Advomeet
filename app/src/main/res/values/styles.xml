<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowDisablePreview">true</item>

    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="cus_deviders">
        <item name="android:background">@color/line_color</item>
        <item name="android:layout_marginLeft">23dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginRight">23dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="cus_deviders_setting">
        <item name="android:background">@color/line_color</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="Theme.Transparent" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="Progressbar.White" parent="AppTheme">
        <item name="colorControlActivated">@color/colorWhite</item>
    </style>

    <style name="cus_devider">
        <item name="android:background">@color/line_color</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginRight">23dp</item>
        <item name="android:layout_marginLeft">23dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="DialogTheme" parent="Theme.AppCompat.DayNight.Dialog.MinWidth">
        <item name="colorAccent">@color/colorPrimary</item>
    </style>
</resources>
